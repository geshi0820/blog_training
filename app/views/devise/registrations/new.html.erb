<%= stylesheet_link_tag 'session' %>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
<%= devise_error_messages! %>
<div id="form-main">
  <div id="form-div">
    <form class="form" id="form1">

      <p class="name">
       <input for="user_name" name="user[username]" type="text" class="validate[required,custom[onlyLetter],length[0,100]] feedback-input" placeholder="Name" id="user_username" />
     </p>

     <p class="email">
      <input for="user_email" name="user[email]" type="text" class="validate[required,custom[onlyLetter],length[0,100]] feedback-input" placeholder="Email" id="user_useremail" />
    </p>

    <p class="password">
      <input for="user_password" name="user[password]" type="text" class="validate[required,custom[onlyLetter],length[0,100]] feedback-input" placeholder="Password" id="user_password" />

    </p>
    
    <div class="submit">
      <input type="submit" value="REGIST" id="button-blue"/>
      <div class="ease"></div>
    </div>
  </form>
</div>
<% end -%>






