<div class="container" style="padding:20px 0">
<%= link_to "お気に入り一覧", favorite_index_articles_path %>
<%= link_to "フォロー一覧", follow_index_articles_path %>
	<table class="table table-striped">
		<thead>
			<tr>
				<th>Title</th>
				<th>Time</th>
				<th>Show</th>
				<th>Drop</th>
			</tr>
		</thead>
		<% @articles.reverse_each do |article| %> 
		<tbody>	
			<tr>
				<td><%=article.title %></td>
				<td><%= article.title %></td>
				<td><%= tokyotime(article.created_at) %></td>
				<td><%= link_to "show", article_path(article),method: :get-%></td>
				<td><%= link_to "delete", article_path(article), method: :delete -%></td>
			</tr>
		</tbody>
		<% end -%>
	</table>
</div>





