<div class="container" style="padding:20px 0">
	<table class="table table-striped">
		<thead>
			<tr>
				<th>Name</th>
				<th>Unfollow</th>
			</tr>
		</thead>
		<% @follow_id.each do |follow| %>
		<tbody>	
			<tr>
				<td><%= @users.find(follow.followed_id).username %></td>
				<td><%= link_to "unfollow", follow_path(follow.follow_id(follow.followed_id, current_user.id),method: :delete %></td>
			</tr>
		</tbody>
		<% end -%>
	</table>
</div>
