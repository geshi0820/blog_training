<div class="container" style="padding:20px 0">
	<table class="table table-striped">
		<thead>
			<tr>
				<th>Name</th>
				<th>Unfollow</th>
			</tr>
		</thead>
		<% @follow_id.each do |follow| %>
		<tbody>	
			<tr>
				<th><%= @user.find(follow.followed_id).username %></th>
				<th><%= link_to "unfollow", follow_path(@follows.where("user_id = ? and followed_id = ?", @userid , article.user_id).pluck(:id).at(0)),method: :delete %></th>
			</tr>
		</tbody>
		<% end -%>
	</table>
</div>
