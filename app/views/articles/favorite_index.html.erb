<div class="container" style="padding:20px 0">
	<table class="table table-striped">
		<thead>
			<tr>
				<th>Title</th>
				<th>Time</th>
				<th>Show</th>
				<th>Edit</th>
				<th>Drop</th>
			</tr>
		</thead>
<% @article_id.each do |f| %>
		<tbody>	
			<tr>
				<th><%= @article.find(f).title %></th>
				<th><%= @article.find(f).created_at %></th>
				<th><%= link_to "show", article_path(f),method: :get-%></th>
				<th><%= link_to "edit", edit_article_path(f),method: :get -%></th>
				<th><%= link_to "delete", article_path(f), method: :delete -%></th>
			</tr>
		</tbody>
		<% end -%>
	</table>
</div>
